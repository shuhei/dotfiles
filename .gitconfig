[color]
  ui = auto
[core]
  excludesfile = ~/dotfiles/.gitignore_global
  editor = vim
  quotepath = false
[push]
	default = simple
[alias]
  s  = status
  ci = commit
  co = checkout
  b  = branch
  l  = log
  a  = add -p
  pl = pull
  ps = push
  d  = diff
  dc = diff --cached
  g  = grep -n
[github]
	user = shuhei
[diff "office"]
  textconv = ~/dotfiles/bin/tika
