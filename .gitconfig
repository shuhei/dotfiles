[include]
  path = ~/.gitconfig.local
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore.global
  editor = vim
  quotepath = false
[alias]
  s  = status
  ci = commit
  co = checkout
  b  = branch
  l  = log
  a  = add -p
  pl = pull
  ps = push
  d  = diff
  dc = diff --cached
  g  = grep -n
[github]
	user = shuhei
